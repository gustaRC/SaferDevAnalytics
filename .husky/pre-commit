BRANCH_NAME=$(git rev-parse --abbrev-ref HEAD)

if [[ "$BRANCH_NAME" == "master" || "$BRANCH_NAME" == "main" ]]; then
    echo "============================================================"
    echo "============ Branch 'Master/Main' identificada! ============"
    echo "========= Versão e CHANGELOG.md serão atualziados. ========="
    echo "============================================================"
fi
